<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="DependencyGraphExtractor.actionSet">
         <action
               class="nz.ac.massey.cs.barrio.srcgraphbuilder.ExtractDependencyGraphAction"
               icon="icons/sample.gif"
               id="barrio.SrcCodeReadAction"
               label="&amp;Analyse Projects in Workspace"
               menubarPath="barrio.topMenu/barrio.SrcCodeReadAction"
               style="push"
               tooltip="Extract dependency graph from selected Java project"/>
      </actionSet>
   </extension>

</plugin>
